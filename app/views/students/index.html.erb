<h1>All student in db</h1>

<table class="table">
  <thead>
  <tr>
    <th> Students</th>
  </tr>
  </thead>

  <tbody>
  <% @students.each do |student| %>>
    <tr>
      <td>
        <%= student.first_name %>>
      </td>
      <td>
        <%= student.last_name %>>
      </td>
      <td>
        <%= student.email %>>
      </td>
      <td>
        <%= student.created_at%>>
      </td>
      <td>
        <%= link_to 'edit', edit_student_path(student) %>>
      </td>
      <td>
        <%= link_to "show", student_path(student)  %>
      </td>
      <td>
        <%= button_to 'Destroy', student_path(student), :method=> :delete, data:{confirm: 'are u sure?'} %>>
      </td>

    </tr>
  <% end %>>
  </tbody>
</table>
<%= link_to 'new', new_student_path(@student) %>