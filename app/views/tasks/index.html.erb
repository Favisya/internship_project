<h1>All task in db</h1>

<table class="table">
  <thead>
  <tr>
    <th> Tasks</th>
  </tr>
  </thead>

  <tbody>
  <% @tasks.each do |task| %>>
    <tr>
      <td>
        <%= task.title %>>
      </td>
      <td>
        <%= task.created_at%>>
      </td>
      <td>
        <%= link_to 'edit', edit_task_path(task) %>>
      </td>
      <td>
        <%= link_to "show", task_path(task)  %>
      </td>
      <td>
        <%= button_to 'Destroy', task_path(task), :method=> :delete, data:{confirm: 'are u sure?'} %>>
      </td>

    </tr>
  <% end %>>
  </tbody>
</table>
<%= link_to 'new', new_task_path(@task) %>